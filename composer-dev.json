{
  "name": "hametuha/hametuha",
  "description": "A WordPress theme for hametuha",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Hametuha INC.",
      "email": "info@hametuha.co.jp"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../../lib/hashboard",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../lib/HamePub",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../../lib/wpametu",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "hametuha/wpametu": "0.8.x-dev",
    "hametuha/HamePub": "1.0.*-dev",
    "hametuha/hashboard": "0.8.x-dev"
  },
  "autoload": {
    "psr-0": {
      "Hametuha": "src"
    }
  }
}
